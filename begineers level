// finding the first and last number of the digit
import java.util.*;
class main 
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int first =n;
        while(first>=10)
        {
            first/=10;
        }
        
        int last=n;
        
        last=last%10;
        System.out.print(first);
        System.out.print(last);
        
    }
}
//input 123457
//output 1 7
