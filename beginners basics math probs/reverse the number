// reverse of the number 
import java.util.*;
class main 
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int rev=0;
        int rem;
        while(n>0)
        {
            rem=n%10;
            rev=(rev*10)+rem;
            n/=10;
        }
        System.out.print(rev);
    }
}
//input = 1234567
//output = 7654321
