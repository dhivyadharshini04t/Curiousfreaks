  
    public int evenlyDivides(int N) {
       int original = N;
       int count=0;
       while(N>0)
       {
       int digit = N %10;
       if(digit !=0 && original%digit==0)
       {
           count++;
       }
       N/=10;
    }
    return count;
    }

  //n = 12
// 12 is divide by 1 also and 12 is divide by 2 also 
// so count as 2 
//output is also 2
